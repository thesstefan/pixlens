{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug GroundedSAM",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pixlens/cli/pixlens_eval_cli.py",
            "args": [
                "--object-detection",
                "GroundedSAM",
                "--image",
                "editval_instances/backpack/000000163866.jpg",
                "--prompt",
                "suitcase . backpack",
                "--out",
                "${workspaceFolder}/results/result_suitcase_backpack_grounded_sam.png"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false,
            "jinja": false,
            "justMyCode": true
        },
        {
            "name": "Debug OwL-ViT+SAM",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pixlens/cli/pixlens_eval_cli.py",
            "args": [
                "--object-detection",
                "Owl-Vit+SAM",
                "--image",
                "editval_instances/backpack/000000163866.jpg",
                "--prompt",
                "suitcase,backpack",
                "--out",
                "${workspaceFolder}/results/result_suitcase_backpack_owlvit_sam.png"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false,
            "jinja": false,
            "justMyCode": true
        },
        {
            "name": "Pixlens Edit CLI - Controlnet",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pixlens/cli/pixlens_edit_cli.py",
            "console": "integratedTerminal",
            "args": [
                "--edit-model",
                "controlnet",
                "--output",
                "${workspaceFolder}/results/edit_output_controlnet.png",
            ]
        },
        {
            "name": "Pixlens Edit CLI - Pix2pix",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pixlens/cli/pixlens_edit_cli.py",
            "console": "integratedTerminal",
            "args": [
                "--edit-model",
                "InstructPix2Pix",
                "--output",
                "${workspaceFolder}/results/edit_output_pix2pix.png",
            ]
        },
        {
            "name": "Debug PixLens Evaluation Pipeline",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pixlens/cli/pixlens_eval_pipeline_cli.py",
            "args": [
                "--edit-id",
                "17", // Replace with the desired edit ID
                "--editing-model",
                "pix2pix", // Replace with the desired editing model name
                "--detection-model",
                "groundedsam" // Replace with the desired detection model name
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false,
            "jinja": false,
            "justMyCode": true
        }
    ]
}