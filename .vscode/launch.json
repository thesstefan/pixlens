{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug GroundedSAM",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pixlens/cli/pixlens_eval_cli.py",
            "args": [
                "--model",
                "GroundedSAM",
                "--image",
                "editval_instances/backpack/000000163866.jpg",
                "--prompt",
                "suitcase . backpack",
                "--out",
                "${workspaceFolder}/results/result_suitcase_backpack_grounded_sam.png",
                "--model_params_yaml",
                "model_cfgs/grounded_sam.yaml",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false,
            "jinja": false,
            "justMyCode": true
        },
        {
            "name": "Debug OwL-ViT+SAM",
            "type": "python",
